{
  "name": "release-port",
  "type": "module",
  "version": "0.0.1-beta.5",
  "packageManager": "pnpm@9.1.2",
  "description": "Kill process running on given port",
  "author": "dmZhan <zqj1130123899@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/dmZhan",
  "homepage": "https://github.com/dmZhan/release-port#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmZhan/release-port.git"
  },
  "bugs": "https://github.com/dmZhan/release-port/issues",
  "keywords": [
    "stop process",
    "cli"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "bin/cli.mjs",
  "files": [
    "dist",
    "logo.png"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "update:deps": "taze major -w -r && pnpm install"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@dmzj/eslint-config": "^0.0.2",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.1",
    "eslint": "^9.1.1",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.0.6",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.7",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
